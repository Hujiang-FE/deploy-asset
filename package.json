    "ftp": "^0.3.10",
    "github-api": "^0.10.6",
    "glob": "^5.0.14",
    "jsonfy": "^0.1.0",
    "lodash": "^3.10.1",
    "mime": "1.3.4",
    "min-asset": "^0.0.3",
